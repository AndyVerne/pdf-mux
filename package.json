{
  "name": "pdf-mux",
  "version": "0.1.1",
  "main": "public/main.js",
  "homepage": "http://ianzen.github.io/pdf-mux",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.11",
    "mousetrap": "^1.6.2",
    "pdfjs-dist": "^2.0.943",
    "react": "^16.7.0",
    "react-beforeunload": "^1.0.4",
    "react-dom": "^16.7.0",
    "react-measure": "^2.2.2",
    "react-pdf": "^4.0.2",
    "react-scripts": "2.1.3",
    "react-split-pane": "^0.1.85",
    "react-window": "^1.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"SET BROWSER=none&&yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "preelectron-pack": "yarn build",
    "electron-pack": "build --c.extraMetadata.main=build/main.js"
  },
  "build": {
    "appId": "com.pdf-mux",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "wait-on": "^3.2.0"
  }
}
